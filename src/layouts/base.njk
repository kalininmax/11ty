{% set siteTitle = "11ty demo" %}
{# {% set navBreadcrumb = collections.all | eleventyNavigationBreadcrumb(eleventyNavigation.key, { includeSelf: true }) | eleventyNavigationToHtml() %} #}

<!DOCTYPE html>
<html lang="ru">
<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title>{{ title }}{% if siteTitle and title %} | {% endif %}{{ siteTitle }}</title>
	<meta name="description" content="{{ description }}">
</head>
<body>
	<header>
		<nav>
			{% navlist collections.all | eleventyNavigation, page, 1 %}

			{# Breadcrumbs #}
			{# {{ navBreadcrumb | safe  }} #}
		</nav>
	</header>
	<main>
		{% block content %}

			<h1>{{ title }}</h1>

			{{ content | safe }}

		{% endblock %}
	</main>
	<footer>
		{% navlist collections.all | eleventyNavigation, page, 2 %}
	</footer>
</body>
</html>